readhtml ::
	(
		html.zip^htmldoczip:
	)
{
	<<<
		mkdir -p /tmp/junkyardhtml
		unzip -oq $(_builddir_)/html.zip -d /tmp/junkyardhtml
		/system/Tracker /tmp/junkyardhtml/index.html
	>>>
}

testphpdoc ::
	(
		php.zip^phpdoczip:
	)
{
	<<<
		sh -c " \
			cd /boot/home/public_html/c2i/html/php && \
			unzip -o $(_builddir_)/php.zip && \
			cvs add * ; \
			cvs commit -m \"\" && \
			dir -al"
	>>>
}

#		dir -al 
